<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
            fx:controller="org.seagrid.desktop.experiment.list.ExperimentListController"
            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
  <children>
    <BorderPane prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <bottom>
        <VBox>
          <children>
            <HBox VBox.margin="$x1">
              <children>
                <CheckBox fx:id="checkAllExps" mnemonicParsing="false" text="Select All">
                  <HBox.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
                  </HBox.margin>
                </CheckBox>
                <Button mnemonicParsing="false" text="Launch Selected" HBox.margin="$x1" />
                <Button mnemonicParsing="false" text="Delete Selected">
                  <HBox.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
                  </HBox.margin>
                </Button>
              </children>
            </HBox>
          </children>
        </VBox>
      </bottom>
      <center>
        <TableView fx:id="expSummaryTable" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="false">
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
          <columns>
            <TableColumn maxWidth="30.0" minWidth="30.0" prefWidth="30.0" text="" fx:id="expCheckedColumn" />
            <TableColumn prefWidth="150.0" text="Experiment  Name" fx:id="expNameColumn" />
            <TableColumn prefWidth="150.0" text="Application" fx:id="expApplicationColumn" />
            <TableColumn prefWidth="150.0" text="Host" fx:id="expHostColumn" />
            <TableColumn prefWidth="150.0" text="Experiment Status" fx:id="expStatusColumn" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="200.0" text="Created Time" fx:id="expCreateTimeColumn" />
          </columns>
        </TableView>
      </center>
    </BorderPane>
  </children>
</AnchorPane>
